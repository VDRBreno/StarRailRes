imo: nivel 10).","materials":[{"id":"18007","num":1}],"level_up_skills":[{"id":"800803","num":2},{"id":"800801","num":1},{"id":"1800701","num":1},{"id":"1800707","num":1}],"icon":"icon/skill/8008_ultimate.png"},{"id":"800806","name":"Portado{F#ra}{M#r} de la revelación divina","rank":6,"desc":"La Prob. CRIT de la habilidad definitiva se fija en el 100%.","materials":[{"id":"18007","num":1}],"level_up_skills":[],"icon":"icon/skill/8008_rank6.png"}],"skills":[{"id":"800801","name":"¡Yo me encargo!","max_level":10,"element":"Ice","type":"Normal","type_text":"ATQ básico","effect":"SingleAttack","effect_text":"ATQ individual","simple_desc":"Inflige una pequeña cantidad de Daño de Hielo a un enemigo.","desc":"Inflige un #1[i]% del ATQ {F#de la}{M#del} Trazacaminos como Daño de Hielo a un enemigo.","params":[[0.5],[0.6],[0.7],[0.8],[0.9],[1],[1.1],[1.2],[1.3],[1.4]],"icon":"icon/skill/8008_basic_atk.png"},{"id":"800802","name":"¡Te elijo a ti!","max_level":15,"element":"","type":"BPSkill","type_text":"Habilidad básica","effect":"Summon","effect_text":"Invocación","simple_desc":"Invoca al mnemoduende Mem. Si Mem ya está en el campo de batalla, Mem regenera PV y obtiene carga.","desc":"Invoca al mnemoduende Mem. Si Mem ya está en el campo de batalla, Mem regenera una cantidad de PV equivalente al #1[i]% de sus PV máx. y obtiene un #2[i]% de carga.","params":[[0.3,0.1],[0.33,0.1],[0.36,0.1],[0.39,0.1],[0.42,0.1],[0.45,0.1],[0.4875,0.1],[0.525,0.1],[0.5625,0.1],[0.6,0.1],[0.63,0.1],[0.66,0.1],[0.69,0.1],[0.72,0.1],[0.75,0.1]],"icon":"icon/skill/8008_skill.png"},{"id":"800803","name":"¡Vamos, Mem!","max_level":15,"element":"Ice","type":"Ultra","type_text":"Habilidad definitiva","effect":"AoEAttack","effect_text":"ATQ AdE","simple_desc":"Invoca al mnemoduende Mem. Mem obtiene carga e inflige una gran cantidad de Daño de Hielo a todos los enemigos.","desc":"Invoca al mnemoduende Mem. Mem obtiene un #2[i]% de carga y luego inflige un #1[i]% de su ATQ como Daño de Hielo a todos los